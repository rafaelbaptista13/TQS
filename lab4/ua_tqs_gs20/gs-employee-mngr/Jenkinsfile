pipeline {
  agent any
  tools {
      jdk 'java-11'
      maven 'maven'
  }
  stages {
      stage('Install') {
          steps {
	      dir('lab4/ua_tqs_gs20/gs-employee-mngr') {
		  sh "$PWD"
		  sh 'mvn clean install'
	      }           
          }
	  post {
	      always {
                   junit '**/target/*-reports/TEST-*.xml'               
	      }
	  }
      }
  }
}

